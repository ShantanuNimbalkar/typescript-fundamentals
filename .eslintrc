{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaFeatures" : {
      "jsx" : true
    },
    "sourceType" : "module"
  },
  "env": {
   "shared-node-browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "no-unused-vars": "error",
    "no-undef": "off",
    "no-redeclare": "off",
    "prefer-const": "off"
  }
}
