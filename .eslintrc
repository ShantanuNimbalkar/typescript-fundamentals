{
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "env": {
    "shared-node-browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "no-unused-vars": "error",
    "no-undef": "off",
    "no-redeclare": "off",
    "prefer-const": "off"
  }
}
